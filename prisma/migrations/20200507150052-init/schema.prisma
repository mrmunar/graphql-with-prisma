generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = "***"
}

model users {
  firstname String
  id        Int    @default(autoincrement()) @id
  lastname  String
  photo_url String
}

model posts {
  id        Int    @default(autoincrement()) @id
  title  String
  content String
}